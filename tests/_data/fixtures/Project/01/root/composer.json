{
    "name": "cheppers/robo-drupal-test-project-01",
    "license": "proprietary",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "bin-dir": "bin",
        "sort-packages": true
    },
    "repositories": {
        "local:drupal/m01": {
            "type": "path",
            "url": "../extensions/m01"
        },
        "local:drupal/m02": {
            "type": "path",
            "url": "../extensions/m02"
        },
        "drupal-8": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    },
    "require": {
        "php": ">=7.1",
        "composer/installers": "^1.0",
        "cweagans/composer-patches": "^1.0",
        "drupal/core": "^8.3",
        "mikey179/vfsStream": "^1.6",
        "phpunit/phpunit": "^5.7"
    },
    "autoload": {
        "psr-4": {
            "Cheppers\\RoboDrupalTestProject01\\": "src/"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "Cheppers\\RoboDrupalTestProject01\\Composer\\Scripts::postInstallCmd"
        ],
        "post-update-cmd": [
            "Cheppers\\RoboDrupalTestProject01\\Composer\\Scripts::postUpdateCmd"
        ],
        "post-create-project-cmd": [
            "Cheppers\\RoboDrupalTestProject01\\Composer\\Scripts::postCreateProjectCmd"
        ]
    },
    "extra": {
        "drupal-scaffold": {
            "excludes": [
                ".csslintrc",
                ".editorconfig",
                ".eslintignore",
                ".eslintrc.json",
                ".gitattributes"
            ]
        },
        "installer-paths": {
            "drupal_root/core": [
                "type:drupal-core"
            ],
            "drupal_root/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "drupal_root/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "drupal_root/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/contrib/{$name}": [
                "type:drupal-drush"
            ]
        }
    }
}
